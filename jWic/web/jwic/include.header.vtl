<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<!-- jWic Application '$context.applicationSetup.name' | http://www.jwic.de -->
<html>
<head>
	<title>#if($page.title)$page.title#else $page.name#end</title>
	<!-- fix for ie parsing problem -->
	<script type="text/javascript">
		var isIE = false;
	</script>
	<!--[if IE]>
		<script type="text/javascript">
		    isIE = true;
		    //any other IE specific stuff here
		</script>
	<![endif]-->
	<META http-equiv="Cache-Control" content="no-cache">
	<META http-equiv="Pragma" content="no-cache">
	<META http-equiv="Expires" content="-1">
	<META http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <script src="$contextPath/jwic/lib/jQuery/js/jquery-1.8.2.js"></script>
    <script src="$contextPath/jwic/lib/jQuery/js/jquery-ui-1.9.1.custom.js"></script> 
    <script src="$contextPath/jwic/lib/jQuery/js/autoNumeric.js"></script>
    <script src="$contextPath/jwic/lib/jQuery/js/jquery.json-2.4.js"></script>
    <script src="$contextPath/jwic/lib/noty/jquery.noty.js"></script>
    <script src="$contextPath/jwic/lib/noty/layouts/top.js"></script>
    <script src="$contextPath/jwic/lib/noty/themes/default.js"></script>
	<script language="JavaScript">
		var _contextPath = "$contextPath";
		var _ajaxMode = $context.applicationSetup.useAjaxRendering;
		jQuery.noConflict();
		
		var JQryEscape = function (str){
			return str.replace(/([;&,\.\+\*\~':"\!\^#$%@\[\]\(\)=>\|])/g, '\\$1');
		};		
		//fix for IE7 parse problem
		//native parsing on IE7 is realy slow
		//jQuery does a check to see if there is a native parser available
		//and uses it if its found, but the one on IE7 its very slow
		jQuery(document).ready(function(){
			if(isIE){
				window.JSON = false;
			}
		});
		
	</script>

	<SCRIPT LANGUAGE="JavaScript" SRC="$contextPath/jwic/calendar/calendar.js"></SCRIPT>
	<SCRIPT LANGUAGE="JavaScript" SRC="$contextPath/jwic/jwic_core.js"></SCRIPT>
	<SCRIPT LANGUAGE="JavaScript" SRC="$contextPath/jwic/jwic_controls.js"></SCRIPT>
	<SCRIPT LANGUAGE="JavaScript" SRC="$contextPath/jwic/jwic_basic_controls.js"></SCRIPT>
	<SCRIPT LANGUAGE="JavaScript" SRC="$contextPath/jwic/jwic_ui.js"></SCRIPT>

	<script language="JavaScript">
		JWic.contextPath = "$contextPath";
	</script>

		
	<!-- <link rel="stylesheet" type="text/css" href="$contextPath/jwic/jwic.css"> -->
	<link rel="stylesheet" type="text/css" href="$contextPath/jwic/jwic_compatibility.css">	
	<link rel="stylesheet" type="text/css" href="$contextPath/jwic/jwic_default.css">	
	<link rel="stylesheet" type="text/css" href="$contextPath/jwic/lib/themes/default.css">
	<link rel="stylesheet" type="text/css" href="$contextPath/jwic/lib/themes/jwicdefault.css">		
	<link rel="stylesheet" type="text/css" href="$contextPath/jwic/lib/themes/j-combo.css">		
	<link rel="stylesheet" type="text/css" href="$contextPath/jwic/lib/jQuery/css/${context.themeName}/jquery-ui.css">	
	